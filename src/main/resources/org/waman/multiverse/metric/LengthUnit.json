{
  "imports":[
    "org.waman.multiverse.time._",
    "org.waman.multiverse.mechanics._",
    "org.waman.multiverse.energy._"
  ],

  "baseUnit":"Metre",
  "baseUnitAccessor":"unitInMetre",

  "multiplicatives":[["LengthUnit", "AreaUnit"], ["ForceUnit", "EnergyUnit"]],
  "divisibles":[["TimeUnit", "VelocityUnit"], ["TimeSquaredUnit", "AccelerationUnit"]],

  "square":"AreaUnit",
  "cubic" :"VolumeUnit",

  "context":"org.waman.multiverse.metric.MetricContext",

  "constants":[
    {"name":"Metre", "symbol":"m", "args":"", "scalePrefixes":true},

    // microscopic
    {"name":"Micron"            , "symbols":["µ", "micron"]  , "args":"r\"1e-6\""},
    {"name":"Angstrom"          , "symbols":["Å", "angstrom"], "args":"r\"1e-10\""},
    {"name":"AtomicUnitOfLength", "symbols":["a0"]           , "args":"r\"5.291772109217e-11\"", "notExact":true},
    {"name":"XUnit"             , "symbols":["xu"]           , "args":"r\"1.0021e-13\"", "notExact":true},
    {"name":"XUnit_CuKAlpha1"   , "symbols":["xu(CuKα1)"]    , "args":"r\"1.0020769928e-13\"", "notExact":true},
    {"name":"XUnit_MoKAlpha1"   , "symbols":["xu(MoKα1)"]    , "args":"r\"1.0020995553e-13\"", "notExact":true},
    {"name":"PlanckLength"      , "symbols":["lp", "l_p"]    , "args":"r\"1.61624e-35\"", "notExact":true},

    // astronomy
    {"name":"AstronomicalUnit", "symbols":["au"], "args":"r\"149597870700\""},
    {"name":"LightYear"       , "symbols":["ly"], "args":"r\"9.4607304725808e15\""},
    {"name":"Parsec"          , "symbols":["pc"], "args":"r\"3.08567782e16\"", "notExact":true},

    // yard-pond
    {"name":"Mil"   , "symbols":["mil", "thou"], "args":"r\"1/1000\", Inch"},
    {"name":"Twip"  , "symbols":["twp"] , "args":"r\"1/20\", Point"},
    {"name":"Point" , "symbols":["pt"]  , "args":"r\"1/72\", Inch"},
    {"name":"Line"  , "symbols":["ln"]  , "args":"r\"1/12\", Inch"},
    {"name":"Inch"  , "symbols":["in"]  , "args":"r\"2.54\", CentiMetre"},
    {"name":"Link"  , "symbols":["li", "lnk"], "args":"r\"0.66\", Foot"},
    {"name":"Foot"  , "symbols":["ft"]  , "args":"12, Inch"},
    {"name":"Yard"  , "symbols":["yd"]  , "args":"3, Foot"},
    {"name":"Ell"   , "symbols":["ell"] , "args":"45, Inch"},
    {"name":"Fathom", "symbols":["ftm"] , "args":"6, Foot"},
    {"name":"Rod"   , "symbols":["rd"]  , "args":"r\"16.5\", Foot"},
    {"name":"Rope"  , "symbols":["rope"], "args":"20, Foot"},
    {"name":"Chain" , "symbols":["ch"]  , "args":"66, Foot"},
    {"name":"Mile"  , "symbols":["mi"]  , "args":"1760, Yard"},
    {"name":"League", "symbols":["lea"] , "args":"3, Mile"},

    {"name":"NauticalMile"          , "symbols":["NM", "nmi"]          , "args":"1852"},
    {"name":"NauticalMile_Admiralty", "symbols":["NM(Adm)", "nmi(Adm)"], "args":"6080, Foot"},
    {"name":"NauticalLeague"        , "symbols":["NL", "nl"]           , "args":"3, NauticalMile"},

    {"name":"Cable"         , "symbols":["cb"]     , "args":"r\"1/10\", NauticalMile"},
    {"name":"Cable_US"      , "symbols":["cb(US)"] , "args":"720, Foot"},
    {"name":"Cable_imperial", "symbols":["cb(imp)"], "args":"608, Foot"},

    {"name":"Link_US_Survey" , "symbols":["li(US)", "lnk(US)"], "args":"r\"0.66\", Foot_US_Survey"},
    {"name":"Foot_US_Survey" , "symbols":["ft(US)"], "args":"r\"1200/3937\""},
    {"name":"Chain_US_Survey", "symbols":["ch(US)"], "args":"66, Foot_US_Survey"},
    {"name":"Mile_US_Survey" , "symbols":["mi(US)"], "args":"5280, Foot_US_Survey"},

    {"name":"MetricFoot"     , "symbols":["mf"] , "args":"Real(r\"1/10\").sqrt()"},
    {"name":"ShortMetricFoot", "symbols":["smf"], "args":"r\"0.3\""},
    {"name":"LongMetricFoot" , "symbols":["lmf"], "args":"r\"1/3\""},

    {"name":"French" , "symbols":["Fr"] , "args":"r\"1/3\", MilliMetre"},
    {"name":"Furlong", "symbols":["fur"], "args":"660, Foot"}
  ]
