{
  "imports":[
    "org.waman.multiverse.metric._",
    "org.waman.multiverse.mass._",
    "org.waman.multiverse.time._",
    "org.waman.multiverse.mechanics._",
    "org.waman.multiverse.fluid._",
    "org.waman.multiverse.electric._",
    "org.waman.multiverse.thermal._",
    "org.waman.multiverse.radiation._",
    "org.waman.multiverse.metric.VolumeUnit._",
    "org.waman.multiverse.fluid.PressureUnit._"
  ],

  "baseUnit":"Joule",
  "baseUnitAccessor":"unitInJoule",

  "multiplicatives":[["TimeUnit", "ActionUnit"]],
  "divisibles":[["TimeUnit", "PowerUnit"], ["TemperatureUnit", "EntropyUnit"], ["MassUnit", "AbsorbedDoseUnit"]],

  "products":[["LengthUnit", "ForceUnit"], ["ChargeUnit", "VoltageUnit"],
             ["PowerUnit", "TimeUnit"], ["VolumeUnit", "PressureUnit"]],

  "context":"org.waman.multiverse.energy.EnergyContext",

  "constants":[
    {"name":"Joule", "symbol":"J", "args":"", "scalePrefixes":true},
    {"name":"Erg", "symbols":["erg"], "args":"r\"1e-7\""},
    {"name":"ElectronVolt", "symbol":"eV", "args":" * ChargeUnit.ElementaryCharge.unitInCoulomb",
      "scalePrefixes":true, "notExact":true},
    {"name":"Rydberg", "symbols":["Ry"], "args":"r\"13.6056925330\", ElectronVolt", "notExact":true},
    {"name":"AtomicUnitOfEnergy", "symbols":["E_h"], "args":"2, Rydberg", "notExact":true},

    // Cal
    {"name":"Calorie_IT"            , "symbols":["cal", "cal(IT)", "cal_IT"], "args":"r\"4.1868\""},
    {"name":"Calorie_thermochemical", "symbols":["cal(th)", "cal_th"]       , "args":"r\"4.184\""},
    {"name":"KiloCalorie"           , "symbols":["kcal", "Cal"]             , "args":"1000, Calorie_thermochemical"},
    {"name":"Calorie_15C"           , "symbols":["cal_15C"]                 , "args":"r\"4.1855\""},

    // BTU
    {"name":"BritishThermalUnit_ISO"           , "symbols":["BTU", "BTU(ISO)", "BTU_ISO"], "args":"r\"1055.056\""},
    {"name":"BritishThermalUnit_IT"            , "symbols":["BTU(IT)", "BTU_IT"]         , "args":"r\"105505585262\""},
    {"name":"BritishThermalUnit_thermochemical", "symbols":["BTU(th)", "BTU_th"]         , "args":"r\"1054.35026444\""},
    {"name":"BritishThermalUnit_59F"           , "symbols":["BTU_59F"]                   , "args":"r\"1054.804\""},

    // Volume*Pressure
    {"name":"LitreAtmosphere"            , "symbols":["l_atm", "ls"]       , "args":"Litre * Atmosphere"},
    {"name":"CubicCentiMetreOfAtmosphere", "symbols":["cc_atm", "scc"]     , "args":"CubicCentiMetre * Atmosphere"},
    {"name":"CubicFootOfAtmosphere"      , "symbols":["cu_ft_atm", "scf"]  , "args":"CubicFoot * Atmosphere"},
    {"name":"CubicYardOfAtmosphere"      , "symbols":["cu_yd_atm", "scy"]  , "args":"CubicYard * Atmosphere"},
    {"name":"GallonAtmosphere_imperial"  , "symbols":["imp_gal_atm"]       , "args":"Gallon_imperial * Atmosphere"},
    {"name":"GallonAtmosphere_US"        , "symbols":["US_gal_atm"]        , "args":"Gallon_US_fluid * Atmosphere"},

    // Others
    {"name":"TonOfTNT", "symbols":["tTNT"], "args":"r\"1e9\", Calorie_IT"},

    {"name":"BarrelOfOilEquivalent", "symbols":["BOE"], "args":"r\"5.8e6\", BritishThermalUnit_59F"},
    {"name":"TonneOfOilEquivalent"  , "symbols":["toe"], "args":"r\"1e10\", Calorie_IT"},
    {"name":"TonOfCoalEquivalent"  , "symbols":["TCE"], "args":"r\"7e9\", Calorie_thermochemical"},
    {"name":"CelsiusHeatUnit", "symbols":["CHU", "CHU(IT)", "CHU_IT"], "args":"r\"1.899100534716e3\""},

    {"name":"Therm_US"  , "symbols":["thm", "thm_US"]    , "args":"r\"1e5\", BritishThermalUnit_59F"},
//    {"name":"Therm_EC"  , "symbols":["thm(EC)", "thm_EC"], "args":"r\"1e5\", BritishThermalUnit_IT? ISO?"},
//    {"name":"Therm_UK"  , "symbols":["thm(UK)", "thm_UK"], "args":"r\"1e5\", BritishThermalUnit_IT?"},
//    {"name":"Thermie", "symbols":["th"], "args":"r\"1e6\", Calorie_IT"},  // conflict to EnergyContext 'th'
    {"name":"Quad", "symbols":["quad"], "args":"r\"1e15\", BritishThermalUnit_IT"}

  ]
}