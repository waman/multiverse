{
  "imports":[
    "org.waman.multiverse.metric._",
    "org.waman.multiverse.mechanics._"
  ],

  "baseUnit":"KiloGram",
  "baseUnitAccessor":"unitInKiloGram",

  "multiplicatives":[["AccelerationUnit", "ForceUnit"]],
  "divisibles":[["VolumeUnit", "DensityUnit"]],

  "constants":[
    {"name":"Gram"   , "symbol":"g"    , "args":" * r\"1e-3\"", "scalePrefixes":true},
    {"name":"Grave"  , "symbols":["gv"], "args":"1, KiloGram"},
    {"name":"Tonne"  , "symbols":["t"] , "args":"1000"},
    {"name":"Gamma"  , "symbols":["Î³"] , "args":"1, MicroGram"},
    {"name":"Quintal", "symbols":["q"] , "args":"100, KiloGram"},

    // microscopic
    {"name":"AtomicMassUnit", "symbols":["u", "AMU", "Da"], "args":"r\"1.66053892173e-27\"", "notExact":true},
    {"name":"ElectronMass"  , "symbols":["m_e"]           , "args":"r\"9.1093829140e-31\"" , "notExact":true},

    // yard-pond
    {"name":"Ounce"      , "symbols":["oz"]            , "args":"28, Gram"},
    {"name":"Pound"      , "symbols":["lb", "lb_av"]   , "args":"r\"0.45359237\""},
    {"name":"LongTon"    , "symbols":["long_tn", "ton"], "args":"2240, Pound"},
    {"name":"ShortTon"   , "symbols":["sh_tn"]         , "args":"2000, Pound"},
    {"name":"Scruple"    , "symbols":["s_ap"]          , "args":"20, Grain"},
    {"name":"Carat"      , "symbols":["kt"]            , "args":"r\"19/6\", Grain"},
    {"name":"MetricCarat", "symbols":["ct"]            , "args":"200, MilliGram"},
    {"name":"Stone"      , "symbols":["st"]            , "args":"14, Pound"},

    // avoirdupois
    {"name":"Dram_avoirdupois"  , "symbols":["dr_av"]          , "args":"27 + r\"11/32\", Grain"},
    {"name":"Grain"             , "symbols":["gr"]             , "args":"r\"1/7000\", Pound"},
    {"name":"LongHundredweight" , "symbols":["cwt", "long_cwt"], "args":"112, Dram_avoirdupois"},
    {"name":"ShortHundredweight", "symbols":["sh_cwt"]         , "args":"100, Dram_avoirdupois"},
    {"name":"Kip"               , "symbols":["kip"]            , "args":"1000, Dram_avoirdupois"},
    {"name":"Ounce_avoirdupois" , "symbols":["oz_av"]          , "args":"r\"1/16\", Pound"},

    // troy
    {"name":"Dram_troy"  , "symbols":["dr_t"]      , "args":"60, Grain"},
    {"name":"Ounce_troy" , "symbols":["oz_t"]      , "args":"r\"1/12\", Pound_troy"},
    {"name":"Pound_troy" , "symbols":["lb_t"]      , "args":"5760, Grain"},
    {"name":"Pennyweight", "symbols":["dwt", "pwt"], "args":"r\"1/20\", Ounce_troy"},

    // others
    {"name":"LongAssayTon" , "symbols":["long_AT", "AT"], "args":"r\"98/3\", Gram"},
    {"name":"ShortAssayTon", "symbols":["sh_AT"]        , "args":"r\"175/6\", Gram"},

    {"name":"Slug", "symbols":["slug"],
      "args":"AccelerationUnit.StandardGravity.unitInMetrePerSecondSquared / LengthUnit.Foot.unitInMetre"}
  ]
}