{
  "imports":[
    "org.waman.multiverse.metric._",
    "org.waman.multiverse.time._",
    "org.waman.multiverse.metric.LengthUnit._",
    "org.waman.multiverse.time.TimeUnit._"
  ],

  "SIUnit":"LengthUnit.Metre / TimeUnit.Second",

  "divisibles":[["TimeUnit", "AccelerationUnit"]],

  "quotients":[["LengthUnit", "TimeUnit"]],

  "context":"org.waman.multiverse.metric.MetricContext",

  "constants":[
