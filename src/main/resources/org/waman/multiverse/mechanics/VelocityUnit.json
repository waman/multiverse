    {"name":"MilePerSecond" , "symbols":["mps"], "args":"Mile / Second"},
    {"name":"MilePerMinute" , "symbols":["mpm"], "args":"Mile / Minute"},
    {"name":"MilePerHour"   , "symbols":["mph"], "args":"Mile / Hour"}
