{
  "imports":[
    "org.waman.multiverse.metric._",
    "org.waman.multiverse.time._",
    "org.waman.multiverse.mass._",
    "org.waman.multiverse.fluid._",
    "org.waman.multiverse.mechanics.AccelerationUnit._",
    "org.waman.multiverse.mass.MassUnit._"
  ],

  "SIUnit":"Newton",

  "multiplicatives":[["LengthUnit", "TorqueUnit"], ["TimeUnit", "MomentumUnit"]],
  "divisibles":[["AreaUnit", "PressureUnit"]],

  "products":[["MassUnit", "AccelerationUnit"]],

  "constants":[
    {"name":"Newton"         , "symbol":"N"     , "args":"", "scalePrefixes":true},
    {"name":"Dyne"           , "symbols":["dyn"], "args":"r\"1e-5\""},
    {"name":"KiloGramForce"  , "symbols":["kgf", "kp", "Gf"],
      "args":"StandardGravity.unitValueInSIUnit", "notExact":true},
    {"name":"MilliGraveForce", "symbols":["mGf", "gf"],
      "args":"StandardGravity.unitValueInSIUnit / 1000", "notExact":true},

    // yard-pond
    {"name":"OunceForce"   , "symbols":["ozf"], "args":"Ounce * StandardGravity"},
    {"name":"PoundForce"   , "symbols":["lbf"], "args":"Pound * StandardGravity"},
    {"name":"Poundal"      , "symbols":["pdl"], "args":"Pound * FootPerSecondSquared"},
    {"name":"KipForce"     , "symbols":["kipf", "klbf"], "args":"1000, Pound * StandardGravity"},
    {"name":"ShortTonForce", "symbols":["sh_tnf"], "args":"ShortTon * StandardGravity"},
    {"name":"LongTonForce" , "symbols":["tnf", "long_tnf"], "args":"LongTon * StandardGravity"},
    {"name":"Sthene"       , "symbols":["sn"], "args":"r\"1e3\""}
  ]
}