{
  "imports":[
    "org.waman.multiverse.metric._",
    "org.waman.multiverse.time._",
    "org.waman.multiverse.mass._",
    "org.waman.multiverse.fluid._",
    "org.waman.multiverse.mechanics.AccelerationUnit._",
    "org.waman.multiverse.mass.MassUnit._"
  ],

  "baseUnit":"Newton",
  "baseUnitAccessor":"unitInNewton",

  "multiplicatives":[["LengthUnit", "TorqueUnit"], ["TimeUnit", "MomentumUnit"]],
  "divisibles":[["AreaUnit", "PressureUnit"]],

  "products":[["MassUnit", "AccelerationUnit"]],

  "constants":[
    {"name":"Newton"         , "symbol":"N"     , "args":"", "scalePrefixes":true},
    {"name":"Dyne"           , "symbols":["dyn"], "args":"r\"1e-5\""},
    {"name":"KiloGramForce"  , "symbols":["kgf", "kp", "Gf"],
      "args":"StandardGravity.unitInMetrePerSecondSquared", "notExact":true},
    {"name":"MilliGraveForce", "symbols":["mGf", "gf"],
      "args":"StandardGravity.unitInMetrePerSecondSquared / 1000", "notExact":true},

    // yard-pond
    {"name":"OunceForce", "symbols":["ozf"],
      "args":"StandardGravity.unitInMetrePerSecondSquared * Ounce.unitInKiloGram"},
    {"name":"PoundForce", "symbols":["lbf"],
      "args":"StandardGravity.unitInMetrePerSecondSquared * Pound.unitInKiloGram"},
    {"name":"Poundal", "symbols":["pdl"],
      "args":"Pound.unitInKiloGram * FootPerSecondSquared.unitInMetrePerSecondSquared"},
    {"name":"KipForce", "symbols":["kipf", "klbf"],
      "args":"StandardGravity.unitInMetrePerSecondSquared * 1000 * Pound.unitInKiloGram"},

    {"name":"ShortTonForce", "symbols":["sh_tnf"],
      "args":"StandardGravity.unitInMetrePerSecondSquared * ShortTon.unitInKiloGram"},
    {"name":"LongTonForce", "symbols":["tnf", "long_tnf"],
      "args":"StandardGravity.unitInMetrePerSecondSquared * LongTon.unitInKiloGram"},
    {"name":"Sthene", "symbols":["sn"], "args":"r\"1e3\""}
  ]
}