{
  "baseUnit":"Second",
  "baseUnitAccessor":"unitInSecond",

  "multiplicatives":[["TimeUnit", "TimeSquaredUnit"]],

  "square":"TimeSquaredUnit",

  "context":"org.waman.multiverse.time.TimeContext",

  "constants":[
    {"name":"Second", "symbol":"s"        , "args":"", "scalePrefixes":true},
    {"name":"Minute", "symbols":["minute"], "args":"60"},
    {"name":"Hour"  , "symbols":["h"]     , "args":"60, Minute"},
    {"name":"Day"   , "symbols":["d"]     , "args":"24, Hour"},
    {"name":"Week"  , "symbols":["wk"]    , "args":"7, Day"},
    {"name":"Year_Gregorian", "symbols":["y", "yr", "a(Gregorian)", "y(Gregorian)", "yr(Gregorian)"],
      "args":"r\"365.2425\", Day"},

    {"name":"Svedberg", "symbols":["S"] , "args":"r\"1e-13\""},
    {"name":"MilliDay", "symbols":["md"], "args":"r\"24\", Hour"},

    {"name":"Decade" , "symbols":["dec"]    , "args":"10, Year_Gregorian"},
    {"name":"Century", "symbols":["century"], "args":"100, Year_Gregorian"},

    {"name":"Jitty"           , "symbols":["j"] , "args":"r\"1/60\", Second"},
    {"name":"JittyAlternative", "symbols":["ja"], "args":"1, CentiSecond"},
    {"name":"Fortnight"       , "symbols":["fn"], "args":"2, Week"},

    // sidereal
    {"name":"Day_sidereal" , "symbols":["d(sidereal)"], "args":"23.9344699, Hour", "notExact":true},
    {"name":"Year_sidereal", "symbols":["y(sidereal)", "yr(sidereal)"],
      "args":"r\"365.256363\", Day", "notExact":true},

    // other years
    {"name":"Year_common"  , "symbols":["a(common)", "y(common)", "yr(common)"], "args":"365, Day"},
    {"name":"Year_leap"    , "symbols":["a(leap)", "y(leap)", "yr(leap)"]      , "args":"366, Day"},
    {"name":"Year_Julian"  , "symbols":["a(Julian)", "y(Julian)", "yr(Julian)"], "args":"r\"365.25\", Day"},
    {"name":"Year_tropical", "symbols":["a(tropical)", "y(tropical)", "yr(tropical)"],
      "args":"r\"365.256363\", Day", "notExact":true}//,

  ]
}