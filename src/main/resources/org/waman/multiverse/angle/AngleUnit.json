{
  "imports":[
    "org.waman.multiverse.MultiverseUtil.twoPi",
    "org.waman.multiverse.time._",
    "org.waman.multiverse.thermal._"
  ],

  "baseUnit":"Radian",
  "baseUnitAccessor":"unitInRadian",

  "divisible":[["TimeUnit", "AngularVelocityUnit"]],

  "constants":[
    {"name":"Radian", "symbol":"rad", "args":"", "scalePrefixes":true,
      "excludePrefixes":["Y", "Z", "E", "P", "T", "G", "M", "k", "h", "da"]},

    {"name":"Degree", "symbols":["deg", "°"], "args":"twoPi / r\"360\"",
      "mixed":"with DegreeTemperaturePostfixOps[TemperatureUnit] { override protected def degreeTemperaturePostfixOps(unit: TemperatureUnit) = unit }"},
    {"name":"Arcmin", "symbols":["arcmin", "MOA"], "args":"r\"1/60\", Degree"},

    {"name":"Arcsec", "symbols":["arcsec"], "args":"r\"1/60\", Arcmin"},
    {"name":"Arcsec", "symbol":"as", "args":", Arcsec", "scalePrefixes":true,
      "excludePrefixes":["Y", "Z", "E", "P", "T", "G", "M", "k", "h", "da", "", "d", "c"]},  // remove 'Arcsec'

    {"name":"Gradian", "symbols":["gon", "ᵍ"], "args":"twoPi / r\"400\""},
    {"name":"Turn"   , "symbols":["tr"]      , "args":"twoPi"}
  ]
}